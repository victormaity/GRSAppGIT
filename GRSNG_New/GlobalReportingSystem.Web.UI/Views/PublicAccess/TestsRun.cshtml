    <body>
        <input type="hidden" id="hidden_user" value="" />
        <div id="history" style="display: none">
        </div>
        @*@{Html.RenderPartial("HistoryDisplayForm");}*@
        <div id="tale">
            <div class="login-container">
                <div class="center">
                    <h1>
                        <i class="ace-icon fa fa-gears red"></i>
                        <span class="red">GRS</span>
                        <span class="infobox-black" id="id-text2">New generation</span>
                    </h1>
                    <h4 class="blue" id="id-company-text">&copy; Clarivate qa automation</h4>
                </div>

                <div class="space-6"></div>

                <div class="position-relative">
                    <div id="login-box" class="login-box visible widget-box no-border">
                        <div class="widget-body">
                            <div class="widget-main">
                                <h4 class="header blue lighter bigger">
                                    <i class="ace-icon fa fa-coffee green"></i>
                                    Please enter your TEN account
                                </h4>

                                <div class="space-6"></div>

                                <fieldset>
                                    <label class="block clearfix">
                                        <span class="block input-icon input-icon-right">
                                            <input class="form-control" id="ten_login" name="Login" placeholder="Username" type="text" value="">
                                            <i class="ace-icon fa fa-user"></i>
                                        </span>
                                    </label>

                                    <label class="block clearfix">
                                        <span class="block input-icon input-icon-right">
                                            <input class="form-control" id="ten_pass" name="Password" placeholder="Password" type="password">
                                            <i class="ace-icon fa fa-lock"></i>
                                        </span>
                                    </label>

                                    <div class="space"></div>

                                    <div class="clearfix">
                                        <button type="submit" class="width-35 pull-right btn btn-sm btn-primary" onclick="checkCredentials();">
                                            @*<a title="Login" href="#modal-form-history" data-action="settings" role="button" data-toggle="modal" style="color: whitesmoke">*@
                                            <i class="ace-icon fa fa-key"></i>
                                            <span class="bigger-110">Login</span>
                                            @*</a>*@
                                        </button>
                                    </div>

                                    <div class="space-4"></div>
                                </fieldset>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </body>

<script>
    function checkCredentials() {
        $('#hidden_user').val($("#ten_login").val());
        $.ajax({
            type: "POST",
            url: "@Url.Action("CheckTenAuthentication", "PublicAccess")",
            async: false,
            data: "{'user':'" + $("#ten_login").val() + "', 'password':'" + $("#ten_pass").val() + "'}",
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            beforeSend: function () { },
            error: function () { },
            success: function (response) {
                if (response != "") {
                    ShowMessage(response.type, response.text);
                } else {
                    $.ajax({
                        type: "POST",
                        url: "@Url.Action("ReturnProjects", "PublicAccess")",
                        async: false,
                        contentType: "application/json; charset=utf-8",
                        dataType: "html",
                        beforeSend: function () { },
                        error: function () { },
                        success: function (msg) {
                            $("#tale").html(msg);
                        }
                    });
                }
            }
        });
    }

    function blockUI() {
        $.blockUI({
            css: {
                border: 'none',
                padding: '15px',
                backgroundColor: '#000',
                '-webkit-border-radius': '10px',
                '-moz-border-radius': '10px',
                opacity: .5,
                color: '#fff'
            }
        });
    }
</script>
